<?xml version="1.0" encoding="UTF-8"?>
<bs:triangles xmlns:bs="https://github.com/borisskert/jaxb-xjb-example"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://github.com/borisskert/jaxb-xjb-example ../xsd/schema.xsd">
    <bs:triangle>
        <bs:name>my 3D triangle</bs:name>
        <bs:a>
            <bs:x>123</bs:x>
            <bs:y>987</bs:y>
            <bs:z>546</bs:z>
        </bs:a>
        <bs:b>
            <bs:x>234</bs:x>
            <bs:y>876</bs:y>
            <bs:z>666</bs:z>
        </bs:b>
        <bs:c>
            <bs:x>345</bs:x>
            <bs:y>765</bs:y>
            <bs:z>000</bs:z>
        </bs:c>
    </bs:triangle>
    <bs:triangle>
        <bs:name>my 2D triangle</bs:name>
        <bs:a>
            <bs:x>123</bs:x>
            <bs:y>987</bs:y>
            <bs:z></bs:z>
        </bs:a>
        <bs:b>
            <bs:x>234</bs:x>
            <bs:y>876</bs:y>
            <bs:z></bs:z>
        </bs:b>
        <bs:c>
            <bs:x>345</bs:x>
            <bs:y>765</bs:y>
            <bs:z></bs:z>
        </bs:c>
    </bs:triangle>
</bs:triangles>
